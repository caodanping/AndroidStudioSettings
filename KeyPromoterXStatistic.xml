<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Change Signature...">
        <Item>
          <IdeaActionID>ChangeSignature</IdeaActionID>
          <ShortCut>'Ctrl+F6'</ShortCut>
          <Description>Change Signature...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Vim Emulator">
        <Item>
          <IdeaActionID>VimPluginToggle</IdeaActionID>
          <ShortCut>'Ctrl+Alt+V'</ShortCut>
          <Description>Vim Emulator</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>